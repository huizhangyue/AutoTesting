<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mappper 3.0 //EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.course.model">
    <!--获取登录接口case-->
    <select id="loginCase" parameterType="Integer" resultType="Integer">
       select * from loginCase
       where id=#{id};
    </select>

    <!--添加用户接口case-->

    <select id="addUserCase" parameterType="Integer" resultType="Integer">
        select * from addUserCase where id = #{id};
    </select>

    <!--获取用户信息case-->

    <select id="getUserInfoCase" parameterType="Integer" resultType="Integer">
        select * from getUserInfoCase where id = #{id};
    </select>

    <!--获取用户列表case-->

    <select id="getUserListCase" parameterType="Integer" resultType="Integer">
        select * from getUserListCase where id = #{id};
    </select>
    <!--更新/删除用户接口case-->

    <select id="updateUserInfoCase" parameterType="Integer" resultType="Integer">
        select * from updateUserInfoCase where id = #{id};
    </select>
</mapper>